extends layout

block content
 form(action="." method="POST")
  input(type="submit" value="Generar Reporte")
 table.table
  thead.thead-dark
   tr
    th Fecha
    th Imagen Base
    th Imagen Modificada
    th Diferencias
    th Observaciones
  tbody
  each val in data
   tr 
    td DD-MM-AAAA
    td
     img(src=`./${val[0]}` style="width:220px")
    td
     img(src=`./${val[1]}` style="width:220px")
    td
     img(src=`./${val[2]}` style="width:220px")
    td Prueba


 